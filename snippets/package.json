{
  "name": "snips",
  "engines": {
    "vscode": "^1.11.0"
  },
  "contributes": {
    "snippets": [
      {
        "language": [
          "css",
          "scss",
          "sass",
          "less",
          "stylus"
        ],
        "path": "./css.json"
      },
      {
        "language": [
          "javascriptreact",
          "typescriptreact"
        ],
        "path": "./react/react-es7.json"
      },
      {
        "language": [
          "plaintex",
          "tex"
        ],
        "path": "./snippets/latex/latex-snippets.json"
      }
    ]
  }
}
